# Copy to .env.docker and fill in secrets before running docker compose

DATABASE_URL=postgresql+asyncpg://context:context@db:5432/context
LLM_PROVIDER=anthropic
ANTHROPIC_API_KEY=change-me

VECTOR_STORE_MODE=milvus
MILVUS_HOST=milvus
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=document_chunks
MILVUS_VECTOR_DIM=768
MILVUS_CONSISTENCY_LEVEL=Bounded

EMBEDDING_VECTOR_DIM=768

# Optional: set if you mount a git repo path into the container
# GIT_REPO_PATH=/app/data/git
